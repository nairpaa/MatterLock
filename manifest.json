{
    "manifest_version": 3,
    "name": "MatterLock",
    "version": "1.0",
    "description": "Enhance your Mattermost experience by securing specific channels with a password.",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [{
        "matches": ["*://web.example.com/*"],
        "js": ["content.js"]
    }],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}